gap =
  
  ##
  element tei:gap {
    model.glossLike*,
    att.global.attribute.xmlid,
    att.global.attribute.n,
    att.global.attribute.xmllang,
    att.global.attribute.rend,
    att.global.attribute.rendition,
    att.global.attribute.xmlbase,
    att.global.linking.attribute.corresp,
    att.global.linking.attribute.synch,
    att.global.linking.attribute.sameAs,
    att.global.linking.attribute.copyOf,
    att.global.linking.attribute.next,
    att.global.linking.attribute.prev,
    att.global.linking.attribute.exclude,
    att.global.linking.attribute.select,
    att.global.analytic.attribute.ana,
    att.global.facs.attribute.facs,
    att.editLike.attribute.cert,
    att.editLike.attribute.resp,
    att.editLike.attribute.evidence,
    att.editLike.attribute.source,
    
    ## donne la raison de l'omission.
    ##                               Les valeurs d'échantillon comprennent
    ##                               échantillonnage,
    ##                               illisible, inaudible,
    ##                                     non pertinent,
    ##                               biffé, biffé et
    ##                               illisible
    attribute reason {
      list {
        xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }+
      }
    }?,
    
    ## lorsque du texte est omis de la
    ##                               transcription en raison d'une suppression volontaire par une main
    ##                               identifiable, indique quelle est cette main
    attribute hand { xsd:anyURI }?,
    
    ## lorsque du texte est omis de la
    ##                               transcription en raison d'un dommage, catégorise la cause du dommage,
    ##                               si celle-ci peut être identifiée
    ## Sample values include: 1] rubbing; 2] mildew; 3] smoke
    attribute agent { xsd:Name }?,
    
    ## indique approximativement la quantité de texte
    ## omise de la transcription (en utilisant l'unité indiquée par l'attribut
    ## unit), à cause de la police éditoriale ou parce qu'un effacement, des
    ## dégâts, ou d'autres raisons ont rendu la transcription impossible
    attribute extent { xsd:double | xsd:decimal }?,
    
    ## nomme l'unité utilisée pour décrire la mesure
    ## de ce qui est effacé
    ## Suggested values include: 1] lines; 2] chars (characters); 3] pages; 4] cm (centimetres); 5] mm (millimetres); 6] in (inches)
    attribute unit {
      
      ## lignes du texte
      "lines"
      | 
        ##  (caractères) caractères du texte
        "chars"
      | 
        ## pages, c'est-à-dire un côté d'une
        ## feuille
        "pages"
      | 
        ##  (centimètres) 
        "cm"
      | 
        ##  (millimètres) 
        "mm"
      | 
        ##  (pouces) 
        "in"
      | xsd:Name
    }?,
    empty
  }
