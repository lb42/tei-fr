    att.global.linking.attribute.sameAs,
    att.global.linking.attribute.copyOf,
    att.global.linking.attribute.next,
    att.global.linking.attribute.prev,
    att.global.linking.attribute.exclude,
    att.global.linking.attribute.select,
    att.global.analytic.attribute.ana,
    att.global.facs.attribute.facs,
    att.ascribed.attribute.who,
    
    ## peut être utilisé pour indiquer
    ##                               si la matière citée est de la parole ou s'il s'agit d'une pensée ou
    ##                               pour la caractériser plus finement
    ## Suggested values include: 1] spoken; 2] thought; 3] written; 4] soCalled; 5] foreign (foreign words); 6] distinct (linguistically distinct); 7] term (technical term); 8] emph (rhetorically emphasized); 9] mentioned
    attribute type {
      
      ## représentation de
      ##                                           discours direct, habituellement marqué par des
      ##                                     guillemets
      "spoken"
      | 
        ## représentation de
        ##                                           pensée, par exemple un monologue interne
        "thought"
      | 
        ## citation à partir
        ##                                           d'une source écrite
        "written"
      | 
        ## distance d'auteur
        "soCalled"
      | 
        ##  (mots étrangers) 
        "foreign"
      | 
        ##  (linguistiquement distinct) 
        "distinct"
      | 
        ##  (terme technique) 
        "term"
      | 
        ##  (rhétoriquement emphatique) 
        "emph"
      | 
        ## référant à lui-même et non pas à son
        ## référant normal
        "mentioned"
      | xsd:Name
    }?,
    empty
  }
