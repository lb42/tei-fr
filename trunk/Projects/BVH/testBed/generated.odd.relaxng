                       name="role"
                       a:defaultValue="data">
               <a:documentation>indicates the kind of information held in this cell or in each cell of this row.</a:documentation>
               <choice>
                  <value>data</value>
                  <a:documentation/>
                  <value>label</value>
                  <a:documentation/>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
