               <group>
                  <ref name="model.common"/>
               </group>
               <zeroOrMore>
                  <ref name="model.global"/>
               </zeroOrMore>
            </zeroOrMore>
            <oneOrMore>
               <choice>
                  <ref name="castItem"/>
                  <ref name="castGroup"/>
               </choice>
               <zeroOrMore>
                  <ref name="model.global"/>
               </zeroOrMore>
            </oneOrMore>
            <zeroOrMore>
               <group>
                  <ref name="model.common"/>
               </group>
               <zeroOrMore>
                  <ref name="model.global"/>
               </zeroOrMore>
