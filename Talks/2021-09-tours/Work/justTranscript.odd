<div type="schemaSpec">
            <head>The transcription ODD specification</head>
   <p>This schema specification supplies the transcription elements we will need in our project
              .</p>
     <schemaSpec ident="justTranscription" start="text" source="motherODD.compiled">
        <moduleRef key="tei"/>
        <moduleRef key="core" include="pb p hi name title name"/>
        <elementSpec ident="name" mode="change">
           <attList>
              <attDef ident="ref" mode="change" usage="req"/>
           </attList>
        </elementSpec>
        <moduleRef key="textstructure" include="body div text"/>
   </schemaSpec>
         </div>
